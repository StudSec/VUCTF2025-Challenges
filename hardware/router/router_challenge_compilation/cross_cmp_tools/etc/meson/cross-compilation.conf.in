# Note: Buildroot's and Meson's terminologies differ about the meaning
# of 'build', 'host' and 'target':
# - Buildroot's 'host' is Meson's 'build'
# - Buildroot's 'target' is Meson's 'host'

[binaries]
c = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/bin/mips-buildroot-linux-uclibc-gcc'
cpp = '/bin/false'
ar = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/bin/mips-buildroot-linux-uclibc-gcc-ar'
strip = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/bin/mips-buildroot-linux-uclibc-strip'
cmake = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/bin/cmake'
fortran = '/bin/false'
pkg-config = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/bin/pkgconf'
g-ir-compiler = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/mips-buildroot-linux-uclibc/sysroot/usr/bin/g-ir-compiler'
g-ir-scanner = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/mips-buildroot-linux-uclibc/sysroot/usr/bin/g-ir-scanner'

[built-in options]
c_args = ['-D_LARGEFILE_SOURCE', '-D_LARGEFILE64_SOURCE', '-D_FILE_OFFSET_BITS=64', '-O2', '-g0'@PKG_TARGET_CFLAGS@]
c_link_args = [@PKG_TARGET_LDFLAGS@]
cpp_args = ['-D_LARGEFILE_SOURCE', '-D_LARGEFILE64_SOURCE', '-D_FILE_OFFSET_BITS=64', '-O2', '-g0'@PKG_TARGET_CXXFLAGS@]
cpp_link_args = [@PKG_TARGET_LDFLAGS@]
fortran_args = ['-O2', '-g0'@PKG_TARGET_FCFLAGS@]
fortran_link_args = [@PKG_TARGET_LDFLAGS@]
wrap_mode = 'nodownload'
cmake_prefix_path = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/mips-buildroot-linux-uclibc/sysroot/usr/lib/cmake'

[properties]
needs_exe_wrapper = true
sys_root = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/mips-buildroot-linux-uclibc/sysroot'
pkg_config_libdir = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/mips-buildroot-linux-uclibc/sysroot/usr/lib/pkgconfig:/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/mips-buildroot-linux-uclibc/sysroot/usr/share/pkgconfig'
pkg_config_static = 'false'
# enable meson build to pass a toolchain file to cmake
cmake_toolchain_file = '/home/joa/github/studsec/vulnerable_mips/buildroot-2025.02/output/host/share/buildroot/toolchainfile.cmake'
cmake_defaults = false

[host_machine]
system = 'linux'
cpu_family = 'mips'
cpu = ''
endian = 'big'
